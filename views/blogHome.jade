doctype html
html
    head
     title= "User Module"
    body
     a(href='/create') Create User
     table(border="1")
      tr
        th= "NAME"
        th= "EMAIL"
        th= "EDIT"
        th= "DELETE"
      each row, index in users
        tr
        td= row.name
        td= row.email
        td
          a(href='/edit/'+row._id+'') Edit
        td
          a(href='/Delete/'+row._id+'',  onclick='return confirm("Are you sure want to delete this user")') Delete          
        
